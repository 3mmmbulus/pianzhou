{
  "name": "wwppcms/site",
  "description": "wwppcms flat-file CMS bundle based on Wwppcms 2.1.4",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "wwppcms/core": "2.1.4",
    "wwppcms/deprecated": "2.1.4",
    "wwppcms/theme-default": "2.1.4",
    "erusev/parsedown": "^1.8",
    "erusev/parsedown-extra": "^0.8",
    "twig/twig": "^1.44",
    "symfony/yaml": "^3.4"
  },
  "autoload": {
    "classmap": [
      "lib/",
      "plugins/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.1.x-dev"
    }
  }
}
