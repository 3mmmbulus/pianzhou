<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for record in sitemaps %}
    <sitemap>
        <loc>{{ record.url|e("html") }}</loc>
        {% if record.modificationTime is not null %}
        <lastmod>{{ record.modificationTime|date("c") }}</lastmod>
        {% endif %}
    </sitemap>
{% endfor %}
</sitemapindex>
